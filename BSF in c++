#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin >> n;
    int parent[n];
    int visited[n];
    int source;
    int arr[n][n];
    queue<int> q;
    for(int i = 0; i < n; i++){
        for(int j = 0; j < n; j++){
            cin >> arr[i][j];
        }
        for(int j = 0; j < n; j++){
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
    cout << "enter Source : ";
    cin >> source;
